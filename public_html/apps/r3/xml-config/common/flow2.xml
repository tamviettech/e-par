	<step order="1" group="BP_MOT_CUA" name="Tiếp nhận hồ sơ" time="1" role="TIEP_NHAN">
		<case flow="basic">
			<task code="TN01::TIEP_NHAN" name="Tiếp nhận hồ sơ" time="1" next="TN01::BAN_GIAO"/>
