<?xml version="1.0"?>
<process code="TN13C" name="Cấp giấy CNQSDĐ đối hộ chuyển nhượng, thừa kế, tặng cho QSD đất và tài sản gắn liền với đất (Trường hợp chỉnh lý biến động )" totaltime="15" version="1" fee="0">
