<?xml version="1.0" encoding="utf-8"?>
<form>
  <line label="Thông tin người nộp hồ sơ" cols="1">
    <item type="TextboxName" id="txtName" name="Người nộp hồ sơ" allownull="no" validate="text" label="Người nộp hồ sơ: " defaul_value="" size="60" css="" Even="ConverUpperCase('txtName',this.value)" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtDiaChi" name="Địa chỉ" allownull="no" validate="text" label="Địa chỉ: " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="DropDownList" id="ddlXaPhuong" name="Phường/Xã" allownull="yes" validate="ddli" label="Phường/Xã:" defaul_value="0" src_file="public/xml/xml_phuong_xa.xml" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtToChucKinhTeHoGiaDinhCaNhan" name="Tổ chức kinh tế hộ, gia đình,cá nhân" allownull="yes" validate="text" label="Tổ chức kinh tế hộ, gia đình,cá nhân: " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtDiachiToChuc" name="Địa chỉ(tổ chức kinh tế, hộ gia đình, cá nhân)" allownull="yes" validate="phone" label="Địa chỉ(tổ chức kinh tế, hộ gia đình, cá nhân):" defaul_value="" size="30" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtDiaChiThuongTru" name="Địa chỉ thường trú" allownull="yes" validate="text" label="Địa chỉ thường trú:" defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtPlaceAdd" name="Địa chỉ thửa đất" allownull="no" validate="text" label="Địa chỉ khu đất: " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtDienTichKhuDat" name="Diện tích khu đất" allownull="yes" validate="number" label="Diện tích khu đất(m2): " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtMucDichSuDung" name="Mục đích sử dụng đất" allownull="yes" validate="text" label="Mục đích sử dụng đất: " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtThoihansudungdat" name="Thời hạn sử dụng đất" allownull="yes" validate="text" label="Thời hạn sử dụng đất: " defaul_value="" size="60" css="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtThanhPho" name="Thành phố" allownull="yes" validate="text" label="Thành phố: " defaul_value="" size="60" css="" view="true" />
  </line>
  <!--tai lieu kem theo don-->
  <line label="Tài liệu kèm theo đơn" cols="1">
    <item type="Checkbox" value="0" doc="yes" id="ckbDonXinChuyenMucDichSuDungDat" name="Tờ khai đăng ký chuyển mục đích sử dụng đất" allownull="" validate="" title="Tờ khai đăng ký chuyển mục đích sử dụng đất" label="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Checkbox" value="1" doc="yes" id="ckbTrichLuc" name="Trích lục bản đồ địa chính, trích lục quy hoạch sử dụng đất chi tiết khu đất" allownull="" validate="" title="Trích lục bản đồ địa chính hoặc trích lục và đo vẽ bổ sung bản đồ địa chính của thửa đất" label="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Checkbox" value="2" doc="yes" id="ckbBanGoc" name="Bản gốc và bản sao Giấy chứng nhận quyền sử dụng đất" allownull="" validate="" title="Giấy chứng nhận quyền sử dụng đất hoặc Giấy chứng nhận quyền sở hữu nhà ở và quyền sử dụng đất ở hoặc Giấy chứng nhận quyền sử dụng đất, quyền sở hữu nhà ở và tài sản khác gắn liền với đất (bản chính)" label="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Checkbox" value="4" doc="yes" id="ckbSoluonghoso" name="Số lượng hồ sơ:  01 bộ" allownull="" validate="" title="Số lượng hồ sơ:  01 bộ " label="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="Checkbox" value="3" id="ckbTaiLieuKhac" name="" allownull="" validate="" title="Tài liệu khác" label="" view="true" />
  </line>
  <line label="" cols="1">
    <item type="TextboxArea" id="txtTaiLieuKhac" name="" allownull="yes" validate="text" label="" defaul_value="" size="50" css="" view="true" />
  </line>
  <!--le phi-->
  <line label="" cols="1">
    <item type="TextboxMoney" id="txtCost" name="Lệ phí tạm thu" allownull="yes" validate="text" label="Lệ phí tạm thu:(đồng)" defaul_value="" size="30" css="" Even="formatCurrency('txtCost',this.value)" />"
 </line>
  <!--button-->
</form>