    <item type="Textbox" id="txtFax" name="Fax" allownull="yes"  validate="fax"   label="Fax:"  defaul_value=""  size="30" css="" />
  </line>
  <!--dang ky kinh doanh voi noi dung sau-->

  <line  label="" cols="2">
