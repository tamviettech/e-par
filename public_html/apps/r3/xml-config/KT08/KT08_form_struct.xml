  <line label="Tài liệu kèm theo đơn" cols="1" groupid="3">

    <item type="Checkbox" value="0" doc="yes"  id="ckbkt1" name="" allownull=""  validate="" title="Đơn đề nghị cấp giấy phép kinh doanh rượu (theo mẫu)" view = "true"/>
  </line>
  <line label="" cols="1" groupid="3">
    <item type="Checkbox" value="1" doc="yes"  id="ckbkt2" name="" allownull=""  validate="" title="Bản sao hợp lệ giấy chứng nhận đăng ký kinh doanh, giấy chứng nhận mã số thuế" label="" view = "true"/>
  </line>
  
  <line label="" cols="1" groupid="3">
    <item type="Checkbox" value="3" doc="yes"  id="ckbkt4" name="" allownull=""  validate="" title="Tài liệu chứng minh quyền sử dụng địa điểm kinh doanh (là chủ sở hữu hoặc đồng sở hữu hoặc thuê sử dụng với thời gian tối thiểu là 01 năm)  " label="" view = "true"/>
  </line>
  <line label="" cols="1" groupid="3">
    <item type="Checkbox" value="4" doc="yes"  id="ckbkt5" name="" allownull=""  validate="" title="Bản kê thiết bị kiểm tra và điều chỉnh nhiệt độ, độ ẩm trong khu vực kinh doanh rượu (để đảm bảo khu vực kinh doanh luôn thoáng mát và chánh được được mặt trời chiếu trực tiếp vào sản phẩm rượu)" label="" view = "true"/>
