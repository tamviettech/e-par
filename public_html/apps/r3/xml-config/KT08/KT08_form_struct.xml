  </line>
  <line label="" cols="1">
    <item type="Textbox" id="txtCapTai" name="Cấp tại" allownull="yes"  validate="text"   label="Cấp tại:"  defaul_value=""  size="60" css="" view = "true"/>
  </line>

  <line label="" cols="1">
    <item type="Textbox" id="txtDuocMuaCua" name="Được mua của các thương nhân bán buôn rượu " allownull="yes"  validate="text"   label="Được mua của các thương nhân bán buôn rượu:"  defaul_value=""  size="60" css="" view = "true"/>
