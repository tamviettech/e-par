  </line>
  
  <line label="" cols="1" groupid="3">
    <item type="Checkbox" value="3" doc="yes"  id="ckbkt5" name="" allownull=""  validate="" title="Các tài liệu liên quan đến an toàn về phóng cháy chữa cháy, về vệ sinh an toàn thực phẩm và bảo vệ môi trường theo quy định" label="" view = "true"/>
  </line>
  
  <line label="" cols="1" groupid="3">
    <item type="Checkbox" value="5" id="ckbTaiLieuKhac" name="" allownull=""  validate="" title="Tài liệu khác" label="" view = "true"/>
  </line>

  <line label="" cols="1">
    <item type="TextboxArea" id="txtTaiLieuKhac" name="" allownull="yes"  validate="text"   label=""  defaul_value=""  size="50" css=""/>
